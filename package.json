    "eslint-plugin-mocha": "^5.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "mocha": "^6.0.1",
    "nyc": "^13.1.0",
    "sinon": "^7.1.1"
  },
  "license": "MIT"
}
